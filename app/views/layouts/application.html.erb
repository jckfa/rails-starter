<!DOCTYPE html>
<html>
  <head>
    <title>One</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all',
                               'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <style>
      .debug_dump {
        background-color: #eee;
        padding: 1em;
        margin: 0;
        position: fixed;
        bottom: 0;
        right: 0;
      }
    </style>
  </head>

  <body>
    <%= debug params if Rails.env.development? %>
    <%= yield %>
  </body>
</html>
